{
    "name": "Hangry Chef",
    "version": "1.0.0",
    "description": "Skip the ads and nonsense and get cooking now!",
    "manifest_version": 3,
    "author": "Brian Yang & Christopher Davis",
    "action":{
        "default_popup": "index.html",
        "default_title": "Hangry Chef Recipe Tracker"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["main.js"]
        }
      ]
}